program appsfv;

uses
  System.StartUpCopy,
  FMX.Forms,
  uPrincipal in 'unit\uPrincipal.pas' {fPrincipal},
  uSplash in 'unit\uSplash.pas' {fSplash},
  uLogin in 'unit\uLogin.pas' {fLogin};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TfPrincipal, fPrincipal);
  Application.CreateForm(TfSplash, fSplash);
  Application.CreateForm(TfLogin, fLogin);
  Application.Run;
end.
